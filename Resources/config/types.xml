<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="wiakowe_form_extensions.form.type.choice_with_disabled_options"
                 class="Wiakowe\FormBundle\Form\Type\ChoiceWithDisabledOptionsType">
            <tag name="form.type" alias="choice_with_disabled_options" />
        </service>

        <service id="wiakowe_form_extensions.form.type.jquery_minicolors"
                 class="Wiakowe\FormBundle\Form\Type\JqueryMinicolorsType">
            <tag name="form.type" alias="jquery_minicolors" />
        </service>
    </services>
</container>
